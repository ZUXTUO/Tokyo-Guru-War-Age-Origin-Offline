<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <gradient android:startColor="#ffffffff" android:endColor="#ffffffff" android:angle="270.0" />
            <corners android:radius="5.0dip" />
            <stroke android:width="1.0px" android:color="#ffd0d6d9" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="#ff999999" android:endColor="#ff999999" android:angle="270.0" />
            <corners android:radius="5.0dip" />
            <stroke android:width="1.0px" android:color="#ffd0d6d9" />
        </shape>
    </item>
</selector>